{
    "name": "elephfront/robo-js-minify",
    "description": "Robo task to perform minification on your JS files.",
    "type": "robo-tasks",
    "license": "MIT",
    "authors": [
        {
            "name": "Yves P.",
            "email": "havokinspiration@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "consolidation/robo": "*",
        "matthiasmullie/minify": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Elephfront\\RoboJsMinify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elephfront\\RoboJsMinify\\Tests\\": "tests/TestCase",
            "Elephfront\\RoboJsMinify\\Tests\\Utility\\": "tests/Utility"
        }
    }
}
